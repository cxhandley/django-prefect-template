# Django
DJANGO_SECRET_KEY=your-secret-key-change-in-production
DJANGO_DEBUG=True
DJANGO_SETTINGS_MODULE=config.settings.development
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# Database (PostgreSQL 18)
DATABASE_URL=postgresql://django:django_password@localhost:5432/django_prefect

# FastAPI Gateway
GATEWAY_JWT_SECRET=gateway-secret-key-change-in-production
GATEWAY_API_URL=http://localhost:8001
GATEWAY_SERVICE_TOKEN=will-be-generated

# Prefect
PREFECT_API_URL=http://localhost:4200/api

# Redis
REDIS_URL=redis://localhost:6379/0

# RustyFS (local S3-compatible storage)
AWS_ACCESS_KEY_ID=rustyfs
AWS_SECRET_ACCESS_KEY=rustyfs_secret
AWS_S3_REGION=us-east-1
DATA_LAKE_BUCKET=django-prefect-datalake-dev
AWS_S3_ENDPOINT_URL=http://localhost:9000

# DuckDB
DUCKDB_THREADS=4
DUCKDB_MEMORY_LIMIT=4GB

# Python
PYTHONUNBUFFERED=1